<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Ryujinx.Rsc"
             x:Class="Ryujinx.Rsc.App"
             xmlns:sty="using:FluentAvalonia.Styling">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
    <Application.Styles>
        <sty:FluentAvaloniaTheme UseSystemThemeOnWindows="False" RequestedTheme="Dark" />
    </Application.Styles>
</Application>
