	<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
		<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="ControlLayout">
			<controls:VirtualStick Width="120" Height="90" Name="StickLeft"/>
			<controls:VirtualStick Width="120" Height="90" Name="StickRight"/>
			<controls:VirtualButton Width="50" Height="50" Name="ButtonA"/>
			<controls:VirtualButton Width="50" Height="50" Name="ButtonB"/>
			<controls:VirtualButton Width="50" Height="50" Name="ButtonX"/>
			<controls:VirtualButton Width="50" Height="50" Name="ButtonY"/>
			<controls:VirtualButton Width="50" Height="50" Name="DpadUp"/>
			<controls:VirtualButton Width="50" Height="50" Name="DPadDown"/>
			<controls:VirtualButton Width="50" Height="50" Name="DPadLeft"/>
			<controls:VirtualButton Width="50" Height="50" Name="DPadRight"/>
			<controls:VirtualButton Width="50" Height="50" Name="Plus"/>
			<controls:VirtualButton Width="50" Height="50" Name="Minus"/>
			<controls:VirtualBumper Width="90" Height="40" Name="LeftShoulder"/>
			<controls:VirtualBumper Width="90" Height="40" Name="RightShoulder"/>
			<controls:VirtualTrigger Width="90" Height="40" Name="LeftTrigger"/>
			<controls:VirtualTrigger Width="90" Height="40" Name="RightTrigger"/>
		</Canvas>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5">
			<Button x:Name="BackButton" Padding="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Click="BackButton_Clicked">
				<PathIcon Height="30" Width="30" Data="{DynamicResource arrow_left_regular}"/>
			</Button>
			<Button x:Name="SaveButton" Padding="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Click="SaveButton_Clicked">
				<PathIcon Height="30" Width="30" Data="{DynamicResource save_regular}"/>
			</Button>
			<Button x:Name="ResetButton" Padding="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Click="ResetButton_Clicked">
				<PathIcon Height="30" Width="30" Data="{DynamicResource arrow_counterclockwise_regular}"/>
			</Button>
			<Button x:Name="DefaultButton" Padding="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Click="DefaultButton_Clicked">
				<PathIcon Height="30" Width="30" Data="{DynamicResource erase_regular}"/>
			</Button>
		</StackPanel>
	</Grid>
</UserControl>
